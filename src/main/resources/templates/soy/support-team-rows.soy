{namespace JIRA.Templates.AssignEscalate}
/**
 * Displays the new team form
 * @param team
 */
{template .supportTeamRows}
  	{foreach $user in $team.users}
  		<tr>
            <td>
                {if $user.assignable}
               		<input CHECKED type="checkbox" class="activatedUsers-{$team.id}" name="activatedUsers" value="{$user.principleName}" >
            
                {else}
                	<input type="checkbox" class="activatedUsers-{$team.id}" name="activatedUsers" value="{$user.principleName}">
            
                {/if}
                </td>
            <td>{$user.displayName} ({$user.principleName})</td>  		
  		</tr>
    {/foreach}  
{/template}